    "lodash.issymbol": "^4.0.1",
    "lodash.mergewith": "^4.6.2",
    "lodash.set": "^4.3.2",
    "lodash.uniq": "^4.5.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/exec": "^6.0.3",
